{
  "version": 2,
  "builds": [
    { "src": "Backend/api/**/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/debug/db", "dest": "/Backend/api/debug/db.js" },
    { "src": "/api/cron/expiry", "dest": "/Backend/api/cron/expiry.js" },
    { "src": "/api/(.*)", "dest": "/Backend/api/index.js" },
    { "src": "/(.*)", "dest": "/Backend/api/index.js" }
  ]
}
